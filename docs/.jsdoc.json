{
    "applicationName": "FallDown",
    "disqus": "",
    "openGraph": {
        "title": "",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "FallDown",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./code/falldown.js",
            "./node_modules/eventemitter3"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "readme": "./README.md",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/jsdoc",
        "template": "./node_modules/yy-jsdoc-template"
    },
    "plugins": [
        "./node_modules/yy-jsdoc-template/plugins/skip-node_modules",
        "plugins/markdown"
    ],
    "readmeShowNav": "FallDown",
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "FallDown",
        "footer"        : "by YOPEY YOPEY LLC (yopeyopey.com)",
        "copyright"     : "Copyright Â© 2020 YOPEY YOPEY LLC.",
        "meta": {
            "title": "FallDown API Documentation",
            "description": "Documentation for falldown library",
            "keyword": "docs, documentation, html5, javascript, jsdoc, drop-down, combo box, options, select, datalist"
        }
    }
}