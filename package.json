{
  "name": "falldown",
  "version": "0.1.0",
  "description": "a vanilla javascript dropbox box replacement with no required external stylesheet",
  "main": "index.js",
  "repository": "https://github.com/davidfig/falldown",
  "author": "David Figatner <david@yopeyopey.com>",
  "license": "MIT",
  "scripts": {
    "test": "budo docs/code.js:index.js --dir docs --live",
    "demo-index": "node docs/generate.js",
    "demo-build": "browserify docs/code | babel-minify -o docs/index.js",
    "demo-css": "cp css/falldown.css docs/",
    "demo": "yarn run docs-css && yarn run docs-index && yarn run docs-build",
    "docs": "jsdoc -c .jsdoc.json"
  },
  "dependencies": {
    "clicked": "^2.1.1",
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "fs-extra": "^7.0.1",
    "yy-jsdoc-template": "^1.3.0"
  },
  "private": false
}
